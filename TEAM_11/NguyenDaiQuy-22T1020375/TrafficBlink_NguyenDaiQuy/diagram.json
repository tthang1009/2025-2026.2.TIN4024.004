    [ "esp:RX", "$serialMonitor:TX", "", [] ],
    [ "esp:23", "r1:1", "red", [ "h0" ] ],
    [ "esp:22", "r2:1", "gold", [ "h0" ] ],
    [ "esp:21", "r3:1", "green", [ "h0" ] ],
    [ "r1:2", "led-red:A", "red", [ "h0" ] ],
    [ "r2:2", "led-yellow:A", "gold", [ "h0" ] ],
    [ "r3:2", "led-green:A", "green", [ "h0" ] ],
    [ "led-red:C", "esp:GND.1", "black", [ "v0" ] ],
    [ "led-yellow:C", "esp:GND.1", "black", [ "v0" ] ],
    [ "led-green:C", "esp:GND.1", "black", [ "v0" ] ]
